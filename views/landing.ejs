    <title>RTC Maps</title>
  </head>

  <body>

<section class="landing-head">
  <image class="background-image" src="/images/landing.jpg"></image>
  <div class="landing-content-area">
    <form>
      <h1>RTC Maps Application</h1>
      <div class="form-group col-md-6 offset-md-3">
        <select class="form-control" id="inputBuilding">
          <option selected="selected">Select Building</option>
          <option>Stuart Building</option>
          <option>Alumini Building</option>
          <option>Idea Shop</option>
          <option>Kaplan Institute</option>
        </select>
      </div>
      <a class="btn btn-info btn-lg" id="openMaps">Open Maps</a>
    </form>
  </div>
</section>

</body>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.6-rc.0/js/select2.min.js"></script>
